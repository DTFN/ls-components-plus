    "vite": "^5.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-files-loader": "^1.1.1",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-simple-html": "^0.1.2",
    "vite-plugin-svg-icons": "^2.0.1",
