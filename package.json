{
  "name": "components-vue3",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:ls-components": "pnpm -r --filter @lingshugroup/web-plus run dev",
    "build:ls-components": "pnpm -r --filter @lingshugroup/web-plus run build",
    "dev:ls-docs": "pnpm -r --filter ls-docs run dev",
    "build:ls-docs": "pnpm -r --filter ls-docs run build",
    "addall": "pnpm install --recursive",
    "prepare": "husky install",
    "lint:fix": "eslint **/*.{vue,ts,json} --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "release": "standard-version",
    "commit": "git add -A && git-cz"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.1.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@lingshugroup/web-plus": "^1.7.1",
    "@univerjs/core": "^0.2.11",
    "@univerjs/design": "^0.2.11",
    "@univerjs/docs": "^0.2.11",
    "@univerjs/docs-ui": "^0.2.11",
    "@univerjs/engine-formula": "^0.2.11",
    "@univerjs/engine-render": "^0.2.11",
    "@univerjs/sheets": "^0.2.11",
    "@univerjs/sheets-formula": "^0.2.11",
    "@univerjs/ui": "^0.2.11",
    "@vue/shared": "^3.5.13",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^12.0.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "colorthief": "^2.4.0",
    "dayjs": "^1.11.13",
    "docx-preview": "^0.3.2",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.1",
    "flv.js": "^1.6.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "luckyexcel": "^1.0.1",
    "pdfjs-dist": "4.8.69",
    "pinia": "^2.2.2",
    "vitepress": "^1.5.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.3",
    "vue3-print-nb": "^0.1.4",
    "vue3-ts-jsoneditor": "^2.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@iconify/vue": "^4.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "axios": "^1.7.7",
    "cz-git": "^1.9.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.19.2",
    "fast-glob": "^3.3.2",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.9",
    "lodash-unified": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-pxtorem": "5.1.1",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "semantic-release": "^24.2.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
